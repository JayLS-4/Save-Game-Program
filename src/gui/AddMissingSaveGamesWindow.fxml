<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="174.0" prefWidth="405.0" type="AnchorPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="lblMissingSaveGame" layoutX="14.0" layoutY="14.0" text="Add missing save game for: ">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="tfPath" layoutX="86.0" layoutY="82.0" prefHeight="25.0" prefWidth="252.0" />
      <Button fx:id="btnBrowse" layoutX="341.0" layoutY="82.0" mnemonicParsing="false" onAction="#btnBrowseOnAction" text="Browse" />
      <Label layoutX="26.0" layoutY="81.0" text="Path:">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Button fx:id="btnAddSaveGame" layoutX="298.0" layoutY="125.0" mnemonicParsing="false" onAction="#btnAddSaveGameOnAction" text="Add save game" />
      <Button fx:id="btnIgnore" layoutX="241.0" layoutY="125.0" mnemonicParsing="false" onAction="#btnIgnoreOnAction" text="Ignore" />
      <Label fx:id="lblPathError" layoutX="86.0" layoutY="107.0" textFill="RED" />
   </children>
</fx:root>
